{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "no-unused-vars": "warn",
        "quotes": ["error", "double"],
        "no-plusplus": "off",
        "no-shadow": "warn",
        "no-underscore-dangle": "warn",
        "prefer-template": "warn",
        "no-undef": "warn",
        "no-restricted-syntax": "warn",
        "no-restricted-globals": "warn",
        "import/prefer-default-export": "off",
        "import/extensions": "off"
    }
}
